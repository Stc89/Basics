            ;流水灯程序
            ;使用杜邦线连接P1与LED端口J9
            
            ORG 00H
            
0000 74FF   START:MOV A,#0FFH    ;赋初值
0002 C3           CLR C
0003 7A08         MOV R2,#8
                  
0005 13     LOOP1:RRC A           ; 带进位右移
0006 F590        MOV P1,A
0008 120019      CALL DELAY
000B DAF8        DJNZ R2,LOOP1
000D 7A07        MOV R2,#7
                 
000F 33     LOOP2:RLC A          ;  带进位左移
0010 F590         MOV P1,A
0012 120019       CALL DELAY
0015 DAF8         DJNZ R2,LOOP2
0017 80E7         JMP START
                  
0019 7B14   DELAY:MOV R3,#20     ;   延时0.2秒
001B 7C14   D1:   MOV R4,#20
001D 7DF8   D2:   MOV R5,#248
001F DDFE         DJNZ R5,$
0021 DCFA         DJNZ R4,D2
0023 DBF6         DJNZ R3,D1
0025 22           RET
                  
            END




















